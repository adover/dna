{
    "name": "silverstripe/installer",
    "type": "silverstripe-recipe",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": ">=5.6.0",
        "silverstripe/recipe-plugin": "^1",
        "silverstripe/recipe-cms": "1.0.1@stable",
        "unclecheese/display-logic": "dev-feature/silverstripe-4",
        "silverstripe/redirectedurls": "dev-feature/silverstripe4",
        "dnadesign/silverstripe-elemental": "2.x-dev",
        "axllent/silverstripe-tiled-gridfield": "^2.0",
        "dnadesign/silverstripe-advanceddropdowns": "^2.0",
        "heyday/silverstripe-responsive-images": "^2.0",
        "dnadesign/silverstripe-dna": "dev-feature/silverstripe4",
        "dnadesign/shipistrano": "dev-master",
        "adrexia/brand": "dev-master",
        "dnadesign/silverstripe-populate": "dev-master",
        "dnadesign/silverstripe-elemental-list": "^1.0@dev",
        "silverstripe/elemental-blocks": "1.0.x-dev",
        "silverstripe/blog": "3.0.x-dev",
        "dnadesign/silverstripe-elemental-userforms": "1.x-dev",
        "undefinedoffset/sortablegridfield": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "repositories": [
        {
          "type": "vcs",
          "url": "https://github.com/adrexia/silverstripe-redirectedurls.git"
        },
        {
          "type": "vcs",
          "url": "https://github.com/adrexia/silverstripe-display-logic.git"
        },
        {
          "type": "vcs",
          "url": "git@github.com:dnadesign/silverstripe-dna.git"
        },
        {
          "type": "vcs",
          "url": "git@github.com:adover/silverstripe-brand.git"
        },
        {
          "type": "vcs",
          "url": "https://github.com/dnadesign/shipistrano.git"
        },
        {
          "type": "vcs",
          "url": "https://github.com/dnadesign/silverstripe-styleguide.git"
        }
    ],
    "extra": {
        "installer-paths": {
          "cap": ["dnadesign/shipistrano"]
        },
        "project-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "mysite/.htaccess",
            "mysite/_config.php",
            "mysite/_config/mysite.yml",
            "mysite/code/Page.php",
            "mysite/code/PageController.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
